# Discord Configuration
DISCORD_TOKEN=your_discord_bot_token_here
DISCORD_GUILD_ID=your_discord_guild_id
DISCORD_BRIDGE_CHANNEL_ID=channel_id_for_bridge_messages
DISCORD_ADMIN_ROLE_ID=optional_admin_role_id
DISCORD_VERIFICATION_CHANNEL_ID=optional_verification_channel_id

# MudVault Mesh Configuration
MUDVAULT_URL=ws://localhost:8081
MUDVAULT_API_KEY=your_mudvault_api_key
MUDVAULT_MUD_NAME=Discord-Bridge
MUDVAULT_CHANNELS=ooc,chat

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Security Configuration
REQUIRE_VERIFICATION=true
RATE_LIMIT_MESSAGES=10
RATE_LIMIT_COMMANDS=5
ALLOWED_MUDS=ExampleMUD,AnotherMUD
BLOCKED_USERS=

# Logging
LOG_LEVEL=info