{
  "name": "mudvault-discord-bridge",
  "version": "1.0.0",
  "description": "Discord bot bridge for MudVault Mesh network",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "typecheck": "tsc --noEmit"
